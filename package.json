    "build": "npm run css && npm run purgecss && lein package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anttti/set.git"
  },
  "author": "Antti Mattila",
  "license": "MIT",
  "homepage": "https://github.com/anttti/set#readme",
  "dependencies": {
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "purgecss": "^1.4.0"
  }
}
