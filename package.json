{
  "name": "set-game",
  "version": "1.0.0",
  "description": "Learning ClojureScript, Reagent and Re-frame by implementing a simple version of the card game Set.",
  "main": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "css": "tailwind build src/styles/styles.css -o public/styles.css",
    "purgecss": "purgecss --config purgecss.config.js --out public",
